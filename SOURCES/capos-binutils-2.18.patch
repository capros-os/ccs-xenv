diff -ruN baseline-binutils-2.18/ld/emulparams/elf_i386_coyotos.sh binutils-2.18/ld/emulparams/elf_i386_coyotos.sh
--- baseline-binutils-2.18/ld/emulparams/elf_i386_coyotos.sh	1969-12-31 19:00:00.000000000 -0500
+++ binutils-2.18/ld/emulparams/elf_i386_coyotos.sh	2007-11-12 23:40:47.000000000 -0500
@@ -0,0 +1,27 @@
+#Link rules variant for Coytos/EROS. This should be the same as elf_i386,
+#except that start address is moved to 0x08000000 so that it will align
+# naturally at a page directory and GPT boundary.
+#
+# I have temporarily disabled PIE and SHLIB, because we aren't using those 
+# and I do not have them debugged.
+SCRIPT_NAME=elf
+OUTPUT_FORMAT="elf32-i386"
+TEXT_START_ADDR=0x08000000
+MAXPAGESIZE=0x1000
+COMMONPAGESIZE=0x1000
+NONPAGED_TEXT_START_ADDR=0x08000000
+ARCH=i386
+MACHINE=
+NOP=0x90909090
+TEMPLATE_NAME=elf32
+DATA_START_SYMBOLS='PROVIDE(__data_start = .);'
+# If this is set to an nonempty string, genscripts.sh will invoke the
+# scripttempl script an extra time to create a shared library script.
+#
+# GENERATE_SHLIB_SCRIPT=
+# If this is set to an nonempty string, genscripts.sh will invoke the
+# scripttempl script an extra time to create a shared library script.
+#
+# GENERATE_PIE_SCRIPT=
+NO_SMALL_DATA=yes
+SEPARATE_GOTPLT=12
